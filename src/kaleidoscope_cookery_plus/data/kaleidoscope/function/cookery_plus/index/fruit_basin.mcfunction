# 【fruit_basin】果盆
data modify storage dc:index input.fruit_basin set value {\
    modsize:[1f,1f,1f],\
    interactsize:{height:0.5f,width:1f},\
    loot_table:"kaleidoscope:cookery_plus/fruit_basin",\
    item:{\
        components:{"minecraft:item_model":"kaleidoscope_cookery_plus:fruit_basin","minecraft:custom_model_data":{"strings":["empty"]}}\
    },\
    template:"kaleidoscope",\
    events:{\
        "construct":[\
            {event:"sound",args:{sound:"minecraft:block.wood.hit"}}\
        ],\
        left_click:{\
            fallback:{event:"destruct",args:{item:{},particle:"block{block_state:\"oak_planks\"}",sound:"minecraft:block.wood.break"}}\
        },\
        right_click:{\
            criteria:[\
                {\
                    event:"trans",\
                    args:{index:"fruit_basin_beetroot",func:"kaleidoscope:cookery_plus/trans/fruit_basin/add_item"},\
                    item:{id:"beetroot"}\
                },\
                {\
                    event:"trans",\
                    args:{index:"fruit_basin_carrot",func:"kaleidoscope:cookery_plus/trans/fruit_basin/add_item"},\
                    item:{id:"carrot"}\
                },\
                {\
                    event:"trans",\
                    args:{index:"fruit_basin_golden_carrot",func:"kaleidoscope:cookery_plus/trans/fruit_basin/add_item"},\
                    item:{id:"golden_carrot"}\
                },\
                {\
                    event:"trans",\
                    args:{index:"fruit_basin_poisonous_potato",func:"kaleidoscope:cookery_plus/trans/fruit_basin/add_item"},\
                    item:{id:"poisonous_potato"}\
                },\
                {\
                    event:"trans",\
                    args:{index:"fruit_basin_potato",func:"kaleidoscope:cookery_plus/trans/fruit_basin/add_item"},\
                    item:{id:"potato"}\
                },\
            ]\
        }\
    }\
}
data modify storage dc:index keylist append value "fruit_basin"

