# 【frogspawn_jelly】蛙籽冻
data modify storage dc:index input.frogspawn_jelly set value { \
    modsize:[1f,1f,1f], \
    interactsize:{height:0.3f,width:1f}, \
    loot_table:"kaleidoscope_meals:frogspawn_jelly", \
    item:{ \
        components:{ \
            "minecraft:item_model":"kaleidoscope_cookery:meals/frogspawn_jelly", \
            "minecraft:custom_model_data":{floats:[0]} \
        }, \
    }, \
    template:"kaleidoscope", \
    events:{ \
        "construct":[ \
            {event:"sound",args:{sound:"minecraft:block.wood.place"}}, \
        ], \
        left_click:{ \
            fallback:{event:"destruct",args:{item:{},particle:"block{block_state:\"oak_planks\"}",sound:"minecraft:block.wood.break"}} \
        }, \
        right_click:{ \
                "fallback": {\
                    "event": "trans",\
                    "args": {\
                        "index": "frogspawn_jelly_1",\
                        "func": "kaleidoscope_meals:effect/eat/frogspawn_jelly"\
                    }\
                }\
            } \
    } \
} 
data modify storage dc:index keylist append value "frogspawn_jelly" 

